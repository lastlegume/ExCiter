{
  "manifest_version": 2,
  "name": "ExCiter",
  "version": "1.0.1",
  "description": "Extracts citations by deleting unwanted bracketed citations and parentheses",
  "icons": {
    "30": "icons/nocitation.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/*", "*://www.fs.usda.gov/database/feis/*"],
      "js": ["remove.js"]
    }
  ],
  "permissions": [
    "storage",
    "activeTab"
  ],
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "browser_action": {
    "default_icon": "icons/nocitation.png",
    "default_title": "Citation Remover",
    "default_popup": "popup/popup.html"
  }
  
  
  
  
  
}